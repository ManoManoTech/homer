    "pg": "8.7.3",
    "pg-hstore": "2.3.4",
    "pino": "7.10.0",
    "prismjs": "1.28.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "sequelize": "6.29.0",
